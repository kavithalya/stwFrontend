{"ast":null,"code":"var __jsx = React.createElement;\nimport React, { useState, useEffect } from 'react';\nimport CommonLayout from '../../../../components/shop/common-layout';\nimport { Container, Row, Form, Label, Input, Col } from 'reactstrap';\nimport firebase, { googleProvider, facebookProvider } from '../../../../config/base';\nimport { toast } from 'react-toastify';\nimport { useRouter } from 'next/router';\n\nconst Login = () => {\n  const router = useRouter();\n  const {\n    0: email,\n    1: setEmail\n  } = useState(\"test@gmail.com\");\n  const {\n    0: password,\n    1: setPassword\n  } = useState(\"test@123\");\n  const {\n    0: name,\n    1: setName\n  } = useState(localStorage.getItem('Name'));\n  useEffect(() => {\n    localStorage.setItem('Name', name);\n  }, [name]);\n\n  const loginAuth = async (email, password) => {\n    try {\n      await firebase.auth().signInWithEmailAndPassword(email, password).then(function () {\n        setName(\"Emay Walter\");\n        setTimeout(() => {\n          router.push(`/page/account/checkout`);\n        }, 200);\n      });\n    } catch (error) {\n      setTimeout(() => {\n        toast.error(\"error\", error);\n      }, 200);\n    }\n  };\n\n  const googleAuth = async () => {\n    try {\n      firebase.auth().signInWithPopup(googleProvider).then(function (result) {\n        setName(result.user.displayName);\n        setTimeout(() => {\n          router.push(`/page/account/checkout`);\n        }, 200);\n      });\n    } catch (error) {\n      setTimeout(() => {\n        toast.error(\"Oppss.. The password is invalid or the user does not have a password.\");\n      }, 200);\n    }\n  };\n\n  const facebookAuth = async () => {\n    try {\n      firebase.auth().signInWithPopup(facebookProvider).then(function (result) {\n        setName(result.user.displayName);\n        setTimeout(() => {\n          router.push(`/page/account/checkout`);\n        }, 200);\n      });\n    } catch (error) {\n      setTimeout(() => {\n        toast.error(\"Oppss.. The password is invalid or the user does not have a password.\");\n      }, 200);\n    }\n  };\n\n  return __jsx(CommonLayout, {\n    parent: \"home\",\n    title: \"login\"\n  }, __jsx(\"section\", {\n    className: \"login-page section-b-space\"\n  }, __jsx(Container, null, __jsx(Row, null, __jsx(Col, {\n    lg: \"6\"\n  }, __jsx(\"h3\", null, \"Login\"), __jsx(\"div\", {\n    className: \"theme-card\"\n  }, __jsx(Form, {\n    className: \"theme-form\"\n  }, __jsx(\"div\", {\n    className: \"form-group\"\n  }, __jsx(Label, {\n    for: \"email\"\n  }, \"Email\"), __jsx(Input, {\n    type: \"text\",\n    defaultValue: email,\n    onChange: e => setEmail(e.target.value),\n    className: \"form-control\",\n    placeholder: \"Email\",\n    required: \"\"\n  })), __jsx(\"div\", {\n    className: \"form-group\"\n  }, __jsx(Label, {\n    for: \"review\"\n  }, \"Password\"), __jsx(Input, {\n    type: \"password\",\n    defaultValue: password,\n    onChange: e => setPassword(e.target.value),\n    className: \"form-control\",\n    id: \"review\",\n    placeholder: \"Enter your password\",\n    required: \"\"\n  })), __jsx(\"a\", {\n    href: \"#\",\n    className: \"btn btn-solid\",\n    onClick: () => loginAuth(email, password)\n  }, \"Login\"), __jsx(\"div\", {\n    className: \"footer-social\"\n  }, __jsx(\"ul\", null, __jsx(\"li\", {\n    onClick: facebookAuth\n  }, __jsx(\"i\", {\n    className: \"fa fa-facebook\",\n    \"aria-hidden\": \"true\"\n  })), __jsx(\"li\", {\n    onClick: googleAuth\n  }, __jsx(\"a\", null, __jsx(\"i\", {\n    className: \"fa fa-google-plus\",\n    \"aria-hidden\": \"true\"\n  })))))))), __jsx(Col, {\n    lg: \"6\",\n    className: \"right-login\"\n  }, __jsx(\"h3\", null, \"New Customer\"), __jsx(\"div\", {\n    className: \"theme-card authentication-right\"\n  }, __jsx(\"h6\", {\n    className: \"title-font\"\n  }, \"Create A Account\"), __jsx(\"p\", null, \"Sign up for a free account at our store. Registrati